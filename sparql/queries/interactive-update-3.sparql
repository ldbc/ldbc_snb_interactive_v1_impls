PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sn: <http://www.ldbc.eu/ldbc_socialnet/1.0/data/>
PREFIX snvoc: <http://www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/>
PREFIX sntag: <http://www.ldbc.eu/ldbc_socialnet/1.0/tag/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dbpedia: <http://dbpedia.org/resource/>
PREFIX dbpedia-owl: <http://dbpedia.org/ontology/>

INSERT
{
    ?person snvoc:likes _:like .
    _:like snvoc:creationDate "$creationDate"^^xsd:dateTime .
    _:like snvoc:hasComment ?comment .
}
WHERE
{
    ?person snvoc:id "$personId"^^xsd:long .
    ?person a snvoc:Person .
    ?comment snvoc:id "$commentId"^^xsd:long .
    ?comment a snvoc:Comment .
}
